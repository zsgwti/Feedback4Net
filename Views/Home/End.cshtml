
@{
    ViewBag.Title = "End";
}


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Dr.coffee 咖博士</title>
    <link href="~/Content/layui/css/layui.css" rel="stylesheet" />
    <script src="~/Content/layui/layui.js"></script>

</head>
<body>

    <header>

        <div class="layui-header" style="width:100%;height:60px;background-color:black">
            <div class="layui-row">
                <div class="layui-col-md3 layui-col-md-offset1">
                    <a href="http://www.dr-coffee.com/"><img src="~/Img/logo_r.png" alt="咖博士 Dr.coffee" style="width: 300px; margin: 20px 0px 0px 0px; text-align: center;" /></a>
                </div>
                <div class="layui-col-md1 layui-col-md-offset6">
                    <div class="layui-row">
                        <div class="layui-col-md3">
                            <a href="/Home/Index"><img src="~/Img/web32.svg" style="margin: 13px 0px 0px 0px" /></a>
                        </div>
                        <div class="layui-col-md1">
                            <div style="margin: 20px 0px 0px 10px; color: white">CN</div>
                        </div>
                        <div class="layui-col-md2">
                            <a href="/Home/Login"><img src="~/Img/user32.svg" style="margin: 13px 0px 0px 30px" /></a>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </header>

    <main>
        <div class="layui-panel" style="width: 30%; height: 20%; margin: 10% 35%;">
            <div class="info" style="margin:40px">
                <div class="img" style="display: flex; justify-content: center; align-items: center;">
                    <img src="~/Img/ok128.svg" alt="" style="margin:40px;" />
                </div>
                <div class="text" style="display: flex; justify-content: center; align-items: center;margin:20px">
                    <h1>Thank you!</h1>
                </div>
                <div class="text" style="display: flex; justify-content: center; align-items: center;">
                    <h3>感谢您的填写，您的宝贵意见是我们前进的动力!</h3>
                </div>
            </div>            
            
        </div>




    </main>

    <footer>
    </footer>







</body>
</html>

